{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/coming-soon", "destination": "/coming-soon.html" },
    { "source": "/new-arrivals", "destination": "/new-arrivals.html" },
    { "source": "/product", "destination": "/product.html" },
    { "source": "/shop", "destination": "/shop.html" },
    { "source": "/cart", "destination": "/cart.html" },
    { "source": "/login", "destination": "/login.html" },
    { "source": "/create-account", "destination": "/create-account.html" },
    { "source": "/members", "destination": "/members.html" },
    { "source": "/updates-welcome", "destination": "/updates-welcome.html" }
  ]
}
