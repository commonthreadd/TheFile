{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/shop", "destination": "/shop.html" },
    { "source": "/cart", "destination": "/cart.html" },
    { "source": "/login", "destination": "/login.html" }
  ]
}
